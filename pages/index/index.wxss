/**index.wxss**/
page {
  height: 100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
}

.wifiList {
  margin-top: 50rpx;
  padding: 20rpx;
  flex: 1;
  overflow-y: scroll;
  box-sizing: content-box;
}

.currentWifi {
  color: #14A513;
  border-bottom: 2rpx solid #dcdcdc;
}

.wifiItem {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
  font-size: 28rpx;
}

.name {
  width: 50%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-right: 10%;
}

.mac {
  flex: 1 0 45%;
  white-space: nowrap;
}

.info {
  padding-top: 20rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}

.setting {
  margin: 20rpx 0;
}